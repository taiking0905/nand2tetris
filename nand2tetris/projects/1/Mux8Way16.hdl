/**
 * 8-way 16-bit multiplexor:
 * out = a if sel = 000
 *       b if sel = 001
 *       c if sel = 010
 *       d if sel = 011
 *       e if sel = 100
 *       f if sel = 101
 *       g if sel = 110
 *       h if sel = 111
 */
CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
       Mux16(a= a, b= b, sel= sel[0] , out= outab0 );
       Mux16(a= c, b= d, sel= sel[0] , out= outcd0 );
       Mux16(a= e, b= f, sel= sel[0] , out= outef0 );
       Mux16(a= g, b= h, sel= sel[0] , out= outgh0 );

       Mux16(a= outab0, b= outcd0, sel= sel[1], out= outabcd1);
       Mux16(a= outef0, b= outgh0, sel= sel[1], out= outefgh1);

       Mux16(a= outabcd1, b= outefgh1, sel= sel[2], out= out);
    //// Replace this comment with your code.
}